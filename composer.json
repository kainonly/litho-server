{
    "name": "laravel/lumen",
    "description": "Van component restful api by Lumen",
    "license": "MIT",
    "type": "project",
    "require": {
        "php": ">=7.1.3",
        "ext-json": "*",
        "ext-pdo": "*",
        "laravel/lumen-framework": "5.8.*",
        "illuminate/cookie": "^5.8",
        "illuminate/redis": "^5.8",
        "barryvdh/laravel-cors": "^0.11.3",
        "ramsey/uuid": "^3.8",
        "ajaxray/short-code": "^1.1",
        "lcobucci/jwt": "^3.3"
    },
    "autoload": {
        "classmap": [
            "database/seeds",
            "database/factories"
        ],
        "psr-4": {
            "App\\": "app/",
            "lumen\\curd\\": "library/lumen-curd/src/",
            "lumen\\extra\\": "library/lumen-extra/src/"
        }
    },
    "scripts": {
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ]
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "repositories": [
        {
            "type": "composer",
            "url": "https://mirrors.aliyun.com/composer"
        }
    ]
}
