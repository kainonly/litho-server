{
    "description": "Open source management infrastructure platform api",
    "type": "project",
    "require": {
        "ext-json": "*",
        "ext-pdo": "*",
        "ext-redis": "*",
        "barryvdh/laravel-cors": "^0.11.3",
        "illuminate/cookie": "^6.3",
        "illuminate/redis": "^6.3",
        "laravel/lumen-framework": "^6.0"
    },
    "require-dev": {
        "ramsey/uuid": "^3.8",
        "lcobucci/jwt": "^3.3"
    },
    "autoload": {
        "classmap": [
            "database/seeds",
            "database/factories"
        ],
        "psr-4": {
            "App\\": "app/",
            "Lumen\\Curd\\": "library/lumen-curd/src/",
            "Lumen\\Extra\\": "library/lumen-extra/src/",
            "Lumen\\Support\\": "library/lumen-support/src/"
        }
    },
    "scripts": {
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ]
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "repositories": {
        "packagist": {
            "type": "composer",
            "url": "https://mirrors.aliyun.com/composer/"
        }
    },
    "license": "MIT"
}
