{
    "description": "Van component restful api by Lumen",
    "license": "MIT",
    "type": "project",
    "require": {
        "ext-json": "*",
        "ext-pdo": "*",
        "laravel/lumen-framework": "^6.0",
        "barryvdh/laravel-cors": "^0.11.3",
        "kain/lumen-curd": "dev-master",
        "kain/lumen-extra": "dev-master",
        "hhxsv5/laravel-s": "^3.5"
    },
    "autoload": {
        "classmap": [
            "database/seeds",
            "database/factories"
        ],
        "psr-4": {
            "App\\": "app/"
        }
    },
    "scripts": {
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ]
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "repositories": {
        "packagist": {
            "type": "composer",
            "url": "https://mirrors.aliyun.com/composer/"
        }
    }
}
