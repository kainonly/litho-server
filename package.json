{
  "name": "van-api",
  "version": "1.0.0",
  "description": "Van component restful api",
  "scripts": {
    "start": "concurrently --handle-input \"wait-on dist/main.js && nodemon\" \"tsc -w -p tsconfig.build.json\" ",
    "start:prod": "node dist/main.js",
    "build": "tsc -p tsconfig.build.json",
    "kill": "taskkill /fi \"imagename eq node.exe\" /f"
  },
  "dependencies": {
    "@nestjs/common": "^6.0.0",
    "@nestjs/core": "^6.0.0",
    "@nestjs/platform-fastify": "^6.2.4",
    "@nestjs/typeorm": "^6.1.1",
    "dotenv": "^8.0.0",
    "ioredis": "^4.9.5",
    "mysql2": "^1.6.5",
    "reflect-metadata": "^0.1.12",
    "rimraf": "^2.6.2",
    "rxjs": "^6.3.3",
    "typeorm": "^0.2.17"
  },
  "devDependencies": {
    "@types/dotenv": "^6.1.1",
    "@types/ioredis": "^4.0.11",
    "@types/node": "^10.12.18",
    "concurrently": "^4.1.0",
    "nodemon": "^1.18.9",
    "prettier": "^1.15.3",
    "ts-node": "8.1.0",
    "tsconfig-paths": "3.8.0",
    "tslint": "5.16.0",
    "typescript": "3.4.3",
    "wait-on": "^3.2.0"
  },
